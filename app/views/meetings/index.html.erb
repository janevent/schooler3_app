<% if params[:course_id] %>
    <h2><%= @course.title %></h2>
<%end%> 
<%= binding.pry %>
<% @meetings.each do |meet| %> 
    <%= meet.start_time %>
    <%= meet.end_time %>
<%end%>  
<h3>Monday</h3>
<% if !@monday_meetings.empty? %>
    <%= @monday_meetings.each do |meet| %>
        <p>Start Time:<%= meet.start_time %> End Time: <%= meet.end_time %></p>
    <%end%>
<%end%>
<h3>Tuesday</h3>
<h3>Wednesday</h3>
<h3>Thursday</h3>
<h3>Friday</h3>
<h3>Saturday</h3>
