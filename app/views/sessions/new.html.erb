<%= render partial: 'layouts/errors', locals: {object:@user} %>
<%= @user.errors[:name] if @user.errors[:name] %>
<%= form_for :user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Sign In" %>
    <%end%>