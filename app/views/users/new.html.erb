<%= render partial: 'layouts/errors', locals: {object:@user} %>
<%= @user.errors.full_messages.first if @user.errors.any? %>
<%= form_for @user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :email_confirmation %>
    <%= f.email_field :email_confirmation %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Create User" %>
    <%end%>